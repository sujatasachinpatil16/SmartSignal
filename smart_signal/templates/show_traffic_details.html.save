{% load static %}
<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN"
        "http://www.w3.org/TR/html4/loose.dtd">
<html>
<head>
    <title>Display list</title>
    <link rel="stylesheet" type="text/css" href="{% static 'styles/main1.css'%}">

    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/css/bootstrap.min.css">
    <link href="https://maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css" rel="stylesheet" />
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/js/bootstrap.min.js"></script>
    <link rel="stylesheet" type="text/css" href="{% static 'styles/main.css'%}">
    <script type="text/javascript" src="{% static 'components/SmartSignalHeader/smartSignalHeader.js'%}"></script>
    <script type="text/javascript" src="{% static 'components/SmartSignalSidebar/smartSignalSidebarAdmin.js'%}"></script> 

</head>
<body>
    <smart-signal-header name="{{user.first_name}}" isAuthenticated="{{user.is_authenticated}}"></smart-signal-header>
    <smart-signal-sidebar-admin></smart-signal-sidebar-admin> 
  <div class="ss-main-container">
  <p>
 <img class="ss-default-bg" src="/static/images/Ecube_logo.png"/>
     <form action="/listout_videos" method="POST">
           {% csrf_token %}

    <div>
            <br><br>
            <table>
 		        <thead>
    		    <tr>
                    <th>Sr no</th>
 		    <th> Date </th>
 	            <th>Road Number</th>
      		    <th>Camera IP</th>
		    <th>Priority</th>
		    <th>Priority ON time</th>
		    <th>Priority OFF time</th>
		    <th>Default green time</th>
		    <th>Green time validity ON time</th>
                    <th>Green time validity OFF time</th>
                    <th>Current green time</th>
                    <th>Want to Update?</th>
   		        </tr>
  		</thead>
			 Location: <b> {{ location }} </b>
			<br><br>		
  		    {% for r in table %}
                <tr >
               		 {% for c in r %}
		                 <td style="text-align:center">{{ c }}</td>
                	 {% endfor %}
                     <td>
                        <input type="submit" name="Update" style="text-align: center;background-color:RED;" value="Update?">
                     </td>
                     <input type="hidden" id="admin_company_nm" name="admin_cnm" value={{c}}>
                 </tr>

        	<input type="hidden" id="company_nm" name="dt" value={{cnm}}>
	        <input type="hidden" id="city_nm" name="rno" value=>
        	{% endfor %}
           </table>
            </div>
        <br> <br>
        
     </form></p>
<br><br>
</div> 
</body>
</html>
